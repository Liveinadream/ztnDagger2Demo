<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fitsSystemWindows="true">

    <include layout="@layout/base_activity_title"
             android:id="@+id/activityTitle"/>

    <Button
            android:id="@+id/sendMessage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/activityTitle"
            android:layout_margin="@dimen/margin_5"
            android:text="@string/send_message"/>

    <TextView
            android:id="@+id/serverReturn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/sendMessage"
            android:layout_margin="@dimen/margin_5"
            android:textColor="@color/black_3e3e3e"/>

    <!--<WebView-->
            <!--android:id="@+id/webView"-->
            <!--android:layout_below="@id/serverReturn"-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="match_parent"/>-->

</RelativeLayout>
